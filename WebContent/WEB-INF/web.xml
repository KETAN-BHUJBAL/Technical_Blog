<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>TechBlog</display-name>
  <servlet>
    <servlet-name>Sign-Up</servlet-name>
    <servlet-class>com.tech.blog.servlets.SignUp</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Sign-Up</servlet-name>
    <url-pattern>/sup</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Log-In</servlet-name>
    <servlet-class>com.tech.blog.servlets.Login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Log-In</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Log-Out</servlet-name>
    <servlet-class>com.tech.blog.servlets.LogOut</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Log-Out</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Edit</servlet-name>
    <servlet-class>com.tech.blog.servlets.EditServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Edit</servlet-name>
    <url-pattern>/editservlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Post</servlet-name>
    <servlet-class>com.tech.blog.servlets.PostServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Post</servlet-name>
    <url-pattern>/addpost</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>LikedServlet</servlet-name>
    <servlet-class>com.tech.blog.servlets.LikedServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LikedServlet</servlet-name>
    <url-pattern>/likeServlet</url-pattern>
  </servlet-mapping>
  
  
  
  
  
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
</web-app>